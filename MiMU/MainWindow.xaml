<Window x:Class="MiMU.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiMU"
        mc:Ignorable="d"
        Title="MiMU" Height="350" Width="525" Loaded="Window_Loaded" Name="UpdaterWindow" Icon="diamond_ore_8Ir_icon.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20*"/>
            <RowDefinition Height="200*"/>
            <RowDefinition Height="20px"/>
            <RowDefinition Height="30px"/>
            <RowDefinition Height="20px"/>
            <RowDefinition Height="30px"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <ProgressBar Grid.Row="3" BorderThickness="0" Name="mainProgress"></ProgressBar>
        <ProgressBar Grid.Row="5" BorderThickness="0" Name="subProgress"></ProgressBar>
        <TextBox Grid.Row="2" Margin="3" BorderThickness="0" Name="mainStatus">MiMU입니다.</TextBox>
        <TextBox Grid.Row="4" Margin="3" BorderThickness="0" Name="subStatus">MiMU입니다.</TextBox>
        <WebBrowser Grid.Row="1" Name="noticeBrowser"></WebBrowser>
    </Grid>
</Window>
